<h1>Edit task: <%= @task.title %></h1>

<%= form_with model: @task, url: task_id_path(@task) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.submit "Update Task" %>
<% end %>
<br>
<%= link_to "Back to list", tasks_path %>
